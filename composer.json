{
    "name": "proyectotau/tau-on-ddd",
    "description": "New TAU version on DDD and agnostic framework",
    "type": "project",
    "license": "GPL-2.0-only",
    "authors": [
        {
            "name": "Javier Garrido",
            "email": "ProyectoTAU2@gmail.com",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "config": {
      "optimize-autoloader": true
    },
    "autoload": {
      "psr-4": {
        "ProyectoTAU\\": "src/"
      },
        "files": [
            "src/TAU/Common/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
          "ProyectoTAU\\Tests\\": "tests/"
        }
     },
    "require": {
        "league/tactician": "2.0.x-dev",
        "league/container": "4.x-dev",
        "ext-pdo": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "8.5.x-dev",
        "mockery/mockery": "^1.5"
    }
}
