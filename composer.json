{
    "name": "proyectotau/tau-on-ddd",
    "description": "New TAU version on DDD and agnostic framework",
    "type": "project",
    "license": "GPL-2.0-only",
    "authors": [
        {
            "name": "Javier Garrido",
            "email": "ProyectoTAU2@gmail.com",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "config": {
      "optimize-autoloader": true
    },
    "autoload": {
        "psr-4": {
          "ProyectoTAU\\": "src/",
          "Joselfonseca\\LaravelTactician\\": "vendor/jagarsoft/laravel-tactician/src/",
          "Joselfonseca\\LaravelTactician\\Tests\\": "vendor/jagarsoft/laravel-tactician/tests/",
          "Tests\\": "tests/"
     },
     "autoload-dev": {
        "psr-4": {
          "Tests\\": "tests/",
          "ProyectoTAU\\Tests\\": "tests/",
          "Joselfonseca\\LaravelTactician\\Tests\\": "vendor/jagarsoft/laravel-tactician/tests/"
        }
     }
    },
    "require": {
      "jagarsoft/laravel-tactician": "1.0.5"
    },
    "require-dev": {
        "phpunit/phpunit": "8.5.x-dev",
        "mockery/mockery": "1.3.x-dev",
        "laravel/framework": "^6.20.12",
        "orchestra/testbench": "^4.0"
    }
}
